//not generic
import java.util.*;
public class CollectionDemo1{
    public static void main(String []args){
        Collection col=new ArrayList();
        col.add("Dinga");
        col.add("Gunda");
        col.add(55.55);
        col.add(true);
        System.out.println(col);
    }
}

[Dinga, Gunda, 55.55, true]

//Generic
import java.util.*;
public class CollectionDemo{
    public static void main(String [] args){
        Collection<String>col=new Vector<>();
        col.add("Cliva");
        col.add("Rhea");
        col.add("Moras");
        System.out.println(col);
    }
}
[Cliva, Rhea, Moras]

public class p2 {
	public static void main(String[] args) {
        for (int i = 1; i <= 2 * 5; i++) {
            int totalColsInRow = i > 5 ? 2 * 5 - i: i;
            for (int j = 1; j <= totalColsInRow; j++) {
                System.out.print("* ");
            }
            System.out.println();
        }
}
}


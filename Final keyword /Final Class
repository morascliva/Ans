// Declaring a final class
final class FinalClass {
    public void display() {
        System.out.println("This is a method in the final class.");
    }
}

// Attempting to inherit from the final class will cause a compile-time error
class SubClass extends FinalClass {  // This will cause a compile-time error
    public void show() {
        System.out.println("This is a method in the subclass.");
    }
}

public class TestFinalClass {
    public static void main(String[] args) {
        FinalClass obj = new FinalClass();
        obj.display();
    }
}

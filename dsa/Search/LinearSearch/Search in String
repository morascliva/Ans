public class SearchinString {
public static void main(String args[]) {
	String name="cliva";
	char target ='i';
	System.out.println(Ssearch(name,target));
}

static boolean Ssearch(String name,char target ) {
	if(name.length()==0) {
		return false;
	}
	for(int i=0;i<name.length();i++) {
		if(target == name.charAt(i))
		{
			return true;
		}
		
	}
	return false;
}
}

public class p2 {
    public static void main(String [] args) {
        int k=1;
        for(int i=1;i<=5;i++) {
            for(int j=1;j<=i;j++) {
                System.out.print((char)(i-j+k+64) + " ");
            }
            k++;
            System.out.println();
        }
    }
}



public class Pattern {
	public static void main(String[] args) {
		int k=1;
		int row =5;
		for(int i=1;i<=row;i++)
		{
			for(int j=1;j<=(row-i);j++) {
				System.out.print(" "+" ");
			}
			for(int j=1;j<=i;j++) {
				System.out.print((char)(k+64) +" ");
				k++;
			}
			System.out.println();
		}
	}

}

       A 
      B C 
    D E F 
  G H I J 
K L M N O 

 
